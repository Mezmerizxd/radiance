{
  "name": "radiance",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "lerna run start --stream --parallel",
    "watch": "lerna run watch --stream --parallel",
    "build": "lerna run build --stream --parallel",
    "test": "lerna run test --parallel",
    "client:start": "lerna run start --scope client --stream",
